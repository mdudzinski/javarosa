<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <h:head>
        <h:title>doc init 2</h:title>
        <model>
            <instance>
                <data id="rgwp">
                    <nodeA/>
                    <group1>
                        <nodeC/>
                        <nodeD/>
                    </group1>
                    <nodeB/>
                    <nodeE>2</nodeE>
                </data>
            </instance>
            <bind nodeset="/data/nodeA" calculate="pow(/data/nodeE, 2)" type="int"/>
            <bind nodeset="/data/nodeB" calculate="pow(/data/nodeE, 2)" type="int" />
            <bind nodeset="/data/group1/nodeC" calculate="pow(/data/nodeE, 2)" type="int" />
            <bind nodeset="/data/group1/nodeD" calculate="pow(/data/nodeE, 2)" type="int" />
            <bind nodeset="/data/nodeE" type="int"/>
        </model>
    </h:head>
    <h:body>
        <input ref="/data/nodeA"/>
        <input ref="/data/nodeB"/>
    </h:body>
</h:html>